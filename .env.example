# OpenAI API Key (Required)
OPENAI_API_KEY=sk-your-key-here

# Database Configuration (Optional - defaults to SQLite)
# DATABASE_URL=postgresql+asyncpg://user:pass@localhost:5432/agent_council
# DATABASE_URL=sqlite+aiosqlite:///./agent_council.db

# Authentication Mode (DEV or PROD)
AUTH_MODE=DEV

# JWT Configuration (Required if AUTH_MODE=PROD)
# AUTH_JWT_SECRET=your-hs256-secret
# AUTH_JWT_PUBLIC_KEY=your-rs256-public-key
# AUTH_JWT_ALG=HS256
# AUTH_JWT_AUDIENCE=api://agent-council
# AUTH_JWT_ISSUER=https://sts.windows.net/...

# Security Controls (Optional)
# DISABLE_UPLOADS=true
# DISABLE_WEB_SEARCH=true

# Logging
LOG_LEVEL=info
